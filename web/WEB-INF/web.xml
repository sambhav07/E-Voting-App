        <servlet-class>evoting.controller.VotingController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddVoteController</servlet-name>
        <servlet-class>evoting.controller.AddVoteController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ElectionResultController</servlet-name>
        <servlet-class>evoting.controller.ElectionResultController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetAllUserDetailsController</servlet-name>
        <servlet-class>evoting.controller.GetAllUserDetailsController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteUserController</servlet-name>
        <servlet-class>evoting.controller.DeleteUserController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterationController</servlet-name>
        <url-pattern>/RegisterationController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/LoginController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminController</servlet-name>
        <url-pattern>/AdminController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddCandidateController</servlet-name>
        <url-pattern>/AddCandidateController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddNewCandidateController</servlet-name>
        <url-pattern>/AddNewCandidateController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowCandidateController</servlet-name>
        <url-pattern>/ShowCandidateController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteCandidateController</servlet-name>
        <url-pattern>/DeleteCandidateController</url-pattern>
    </servlet-mapping>
