        <servlet-class>evoting.controller.RegisterationController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>evoting.controller.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminController</servlet-name>
        <servlet-class>evoting.controller.AdminController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddCandidateController</servlet-name>
        <servlet-class>evoting.controller.AddCandidateController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddNewCandidateController</servlet-name>
        <servlet-class>evoting.controller.AddNewCandidateController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowCandidateController</servlet-name>
        <servlet-class>evoting.controller.ShowCandidateController</servlet-class>
